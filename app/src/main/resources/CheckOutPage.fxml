<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FrontEnd.CheckOutPageController">
   <children>
      <Button fx:id="cashButton" layoutX="177.0" layoutY="289.0" mnemonicParsing="false" onAction="#payByCash" text="Pay By Cash" />
      <Button fx:id="cardButton" layoutX="296.0" layoutY="289.0" mnemonicParsing="false" onAction="#payByCard" text="Pay By Card" />
      <Button fx:id="back" layoutX="83.0" layoutY="289.0" mnemonicParsing="false" onAction="#back" text="Back" />
      <Label fx:id="total" layoutX="201.0" layoutY="42.0" text="Total Price:" />
      <Label fx:id="shoppingCartLabel" layoutX="201.0" layoutY="98.0" text="Shopping Cart:" />
      <Button fx:id="cancel" layoutX="439.0" layoutY="290.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
      <Label fx:id="timer" layoutX="495.0" layoutY="34.0" text="Timer" />
   </children>
</AnchorPane>
