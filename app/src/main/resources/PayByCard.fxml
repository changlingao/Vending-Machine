<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FrontEnd.PayByCardController">
   <children>
      <Label layoutX="105.0" layoutY="121.0" text="Card Holder Name" />
      <Label layoutX="105.0" layoutY="160.0" text="Credit Card Number" />
      <TextField fx:id="name" layoutX="245.0" layoutY="117.0" />
      <Button fx:id="submitButton" layoutX="217.0" layoutY="224.0" mnemonicParsing="false" onAction="#submit" text="Submit" />
      <TextField fx:id="numberFiled" layoutX="245.0" layoutY="156.0" onKeyTyped="#asterisk" />
      <Button fx:id="back" layoutX="105.0" layoutY="224.0" mnemonicParsing="false" onAction="#back" text="Back" />
      <Button fx:id="cancel" layoutX="350.0" layoutY="225.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
      <Label fx:id="timer" layoutX="459.0" layoutY="50.0" text="Timer" />
   </children>
</AnchorPane>
