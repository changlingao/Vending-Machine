<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FrontEnd.LoginPageController">
   <children>
      <Label layoutX="140.0" layoutY="123.0" text="Username" />
      <Label layoutX="143.0" layoutY="165.0" text="Password" />
      <TextField fx:id="username" layoutX="220.0" layoutY="118.0" />
      <Button fx:id="loginButton" layoutX="240.0" layoutY="265.0" mnemonicParsing="false" onAction="#loginOrCreate" text="LoginOrCreate" />
      <TextField fx:id="passwordField" layoutX="220.0" layoutY="160.0" onKeyTyped="#asterisk" />
      <Button fx:id="back" layoutX="168.0" layoutY="265.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
   </children>
</AnchorPane>
